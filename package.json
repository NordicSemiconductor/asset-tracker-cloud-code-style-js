{
  "name": "@bifravst/code-style",
  "version": "0.0.0-development",
  "description": "Code style definitions for all Bifravst projects.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/code-style.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/code-style/issues"
  },
  "homepage": "https://bifravst.github.io/",
  "keywords": [
    "Cloud",
    "IoT",
    "Bifravst",
    "Asset Tracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@bifravst/eslint-config-typescript": "5.0.75",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "eslint": "7.13.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2",
    "snyk": "1.425.2",
    "typescript": "4.0.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{yml,md,js,json}": [
      "prettier --write --config .prettierrc.js"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "index.js",
    "LICENSE",
    "README.md",
    ".prettierrc.js",
    "tsconfig.json",
    "templates"
  ],
  "scripts": {
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "snyk": true
}
