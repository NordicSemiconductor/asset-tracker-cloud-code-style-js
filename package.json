{
  "name": "@bifravst/code-style",
  "version": "0.0.0-development",
  "description": "Code style definitions for all Bifravst projects.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/code-style.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/code-style/issues"
  },
  "homepage": "https://bifravst.github.io/",
  "keywords": [
    "Cloud",
    "IoT",
    "bifravst",
    "AssetTracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@bifravst/eslint-config-typescript": "^4.0.0",
    "eslint": "^7.3.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "snyk": "^1.359.0"
  },
  "peerDependencies": {
    "typescript": "^3.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "husky": "^4.2.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{yml,md,js,json}": [
      "prettier --write --config .prettierrc.js"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "index.js",
    "LICENSE",
    "README.md",
    ".prettierrc.js",
    "tsconfig.json",
    "templates"
  ],
  "scripts": {
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "snyk": true
}
