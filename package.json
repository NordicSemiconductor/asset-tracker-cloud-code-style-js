{
  "name": "@bifravst/code-style",
  "version": "0.0.0-development",
  "description": "Code style definitions for all Bifravst projects.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/code-style.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/code-style/issues"
  },
  "homepage": "https://bifravst.github.io/",
  "keywords": [
    "Cloud",
    "IoT",
    "bifravst",
    "AssetTracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@bifravst/eslint-config-typescript": "^3.0.0",
    "eslint": "^6.4.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "husky": "^3.0.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{yml,md,js,json}": [
      "prettier --write --config .prettierrc.js",
      "git add"
    ]
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branch": "saga",
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "index.js",
    "LICENSE",
    "README.md",
    ".prettierrc.js",
    "tsconfig.json",
    "templates"
  ]
}
